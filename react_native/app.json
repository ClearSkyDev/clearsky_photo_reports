{
  "expo": {
    "name": "ClearSky",
    "slug": "clearsky-photo-reports",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0E5CC8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.clearsky.photo",
      "buildNumber": "1.0.2",
      "infoPlist": {
        "NSCameraUsageDescription": "ClearSky needs access to your camera to capture roof and property photos.",
        "NSPhotoLibraryUsageDescription": "ClearSky needs access to your photo library to select and attach existing images.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.clearsky.photo",
      "versionCode": 3,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0E5CC8"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {}
      ],
      [
        "expo-image-picker",
        {}
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
