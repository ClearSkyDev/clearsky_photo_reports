{
  "expo": {
    "name": "ClearSky Photo Reports",
    "slug": "clearsky_photo_reports",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "sdkVersion": "53.0.0",
    "ios": {
      "bundleIdentifier": "com.clearsky.photo",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow ClearSky Photo Reports to use the camera for capturing roof inspection photos.",
        "NSPhotoLibraryUsageDescription": "Allow ClearSky Photo Reports to access your photo library for uploading inspection photos.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.clearsky.photo",
      "versionCode": 1
    },
    "extra": {
      "eas": {
        "projectId": "3722841f-e742-4d43-859c-b6c5b0e8d784"
      }
    }
  }
}