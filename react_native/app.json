{
  "expo": {
    "name": "ClearSky Photo Reports",
    "slug": "clearsky-photo-reports",
    "owner": "clearsky",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.clearsky.photo",
      "buildNumber": "1.0.1",
      "supportsTablet": false,
      "infoPlist": {
        "NSCameraUsageDescription": "Used to capture photos for roof inspections.",
        "NSPhotoLibraryUsageDescription": "Used to save and upload photos.",
        "NSMicrophoneUsageDescription": "Optional video capture.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.clearsky.photo",
      "versionCode": 2,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "3722841f-e742-4d43-859c-b6c5b0e8d784"
      }
    },
    "plugins": [
      "expo-camera",
      "expo-file-system",
      "expo-media-library",
      "expo-image-picker",
      "expo-web-browser"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  },
  "cli": {
    "appVersionSource": "auto"
  }
}
